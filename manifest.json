{
    "manifest_version": 2,
    "name": "Jira Template",
    "version": "1.5.0",
    "icons": {
        "128": "128.png"
    },
    "description": "Utiliza as informações do jira para gerar subject emails, branch name",
    "author": "Felipe Reis",
    "content_scripts": [
        {
            "matches": [
                "https://jira.icarotech.com/*"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "permissions": [
        "unlimitedStorage",
        "notifications"
    ]
}